<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#101010"
    android:foregroundTint="#00FFFFFF"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/demo_map"
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/collision"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="100dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/crossing"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/todo" />
    <ImageView
        android:id="@+id/distance"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="170dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        app:srcCompat="@drawable/distance"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/crossing_people"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="230dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/cross_people"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/seatbelt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="70dp"
        android:layout_marginStart="360dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/distance"
        app:srcCompat="@drawable/seatbelt"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>
    <ImageView
        android:id="@+id/door"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="60dp"
        android:layout_marginStart="360dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/seatbelt"
        app:srcCompat="@drawable/door_car"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/left_signal"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="120dp"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"
        app:srcCompat="@drawable/left_signal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/right_signal"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="120dp"
        app:srcCompat="@drawable/right_signal"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/far_light"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        app:srcCompat="@drawable/far_light"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>
    <ImageView
        android:id="@+id/near_light"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="5dp"
        app:srcCompat="@drawable/near_light"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>
    <ImageView
        android:id="@+id/slippery"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        app:srcCompat="@drawable/slippery"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>
    <ImageView
        android:id="@+id/lane"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="100dp"
        app:srcCompat="@drawable/lane"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/demo_map"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>

    <ImageView
        android:id="@+id/letter_d"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/letter_d"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>

    <ImageView
        android:id="@+id/letter_n"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/letter_n"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>
    <ImageView
        android:id="@+id/letter_r"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/letter_r"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>

    <ImageView
        android:id="@+id/letter_p"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/letter_p"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/todo"
        tools:ignore="MissingConstraints"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/dropdown2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:tint="#FFFFFF"
        app:layout_constraintTop_toTopOf="parent"
        app:backgroundTint="#605454"
        app:rippleColor="#E4DEDE"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/down"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/todo"
        android:focusable="true" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:backgroundTint="#605454"
        app:layout_constraintEnd_toEndOf="@id/dropdown2"
        app:layout_constraintTop_toBottomOf="@id/dropdown2"
        app:srcCompat="@drawable/button"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:backgroundTint="#605454"
        app:layout_constraintEnd_toEndOf="@id/but1"
        app:layout_constraintTop_toBottomOf="@id/but1"
        app:srcCompat="@drawable/button2"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:backgroundTint="#605454"
        app:layout_constraintEnd_toEndOf="@id/but2"
        app:layout_constraintTop_toBottomOf="@id/but2"
        app:srcCompat="@drawable/button3"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/dropdown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:tint="#FFFFFF"
        app:layout_constraintTop_toTopOf="parent"
        app:backgroundTint="#605454"
        app:rippleColor="#E4DEDE"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/down"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/todo"
        android:focusable="true" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:backgroundTint="#605454"
        app:layout_constraintEnd_toEndOf="@id/dropdown"
        app:layout_constraintTop_toBottomOf="@id/dropdown"
        app:srcCompat="@drawable/button4"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@id/but4"
        app:layout_constraintTop_toBottomOf="@id/but4"
        app:backgroundTint="#605454"
        app:srcCompat="@drawable/button5"
        tools:layout_editor_absoluteX="679dp"
        tools:layout_editor_absoluteY="160dp"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@id/but5"
        app:layout_constraintTop_toBottomOf="@id/but5"
        app:backgroundTint="#605454"
        app:srcCompat="@drawable/button6"
        tools:layout_editor_absoluteX="679dp"
        tools:layout_editor_absoluteY="160dp"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/but7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@id/but6"
        app:layout_constraintTop_toBottomOf="@id/but6"
        app:backgroundTint="#605454"
        app:srcCompat="@drawable/button7"
        tools:layout_editor_absoluteX="679dp"
        tools:layout_editor_absoluteY="160dp"
        tools:ignore="MissingConstraints"
        android:focusable="true"
        android:contentDescription="@string/todo"
        />


</androidx.constraintlayout.widget.ConstraintLayout>